{
  "name": "zypher-server",
  "version": "1.0.0",
  "description": "Node.js TypeScript server with Supabase integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "type-check": "tsc --noEmit",
    "init-db": "ts-node src/scripts/initDatabase.ts",
    "test-api": "curl http://localhost:3001/api/health"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "supabase",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "ioredis": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/socket.io": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
